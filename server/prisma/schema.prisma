// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model File {
  id           Int    @id @default(autoincrement())
  originalname String
  filename     String
  size         Int
  mimetype     String
}

model Note {
  id   Int    @id @default(autoincrement())
  name String
  text String
}
